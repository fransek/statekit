{
  "name": "fransek-store",
  "version": "2.0.0",
  "author": "Frans Ekman <fransedvinekman@gmail.com>",
  "license": "MIT",
  "description": "A tiny state management library for React.",
  "keywords": [
    "store",
    "state",
    "react",
    "reducer",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fransek/store"
  },
  "bugs": {
    "url": "https://github.com/fransek/store/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "ui-test": "playwright test",
    "coverage": "vitest --run --coverage",
    "demo": "cd demo && pnpm run dev",
    "demo:build": "cd demo && pnpm install && pnpm build",
    "demo:start": "cd demo && pnpm run start",
    "lint": "eslint",
    "prepare": "husky"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "bash -c tsc --noEmit"
    ]
  },
  "dependencies": {
    "superjson": "^2.2.2"
  }
}
